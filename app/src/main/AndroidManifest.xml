<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lize">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Lize">

        <activity android:name=".workers.LogInActivity"></activity>
        <activity android:name=".workers.SignUpActivity"></activity>

        <activity android:name=".workers.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".workers.EditarAmbitoActivity"></activity>
        <activity android:name=".workers.AjustesActivity"></activity>
        <activity android:name=".workers.NotasActivity"></activity>
    </application>

</manifest>